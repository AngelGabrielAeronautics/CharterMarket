# Firebase Configuration (Client)
NEXT_PUBLIC_FIREBASE_API_KEY=your-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-app-id

# Firebase Admin SDK Configuration (Server)
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_CLIENT_EMAIL=your-service-account-email
FIREBASE_PRIVATE_KEY=your-private-key

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Super Admin Account Details
SUPER_ADMIN_EMAIL=your-super-admin-email
SUPER_ADMIN_PASSWORD=your-secure-password
SUPER_ADMIN_FIRST_NAME=Admin
SUPER_ADMIN_LAST_NAME=Super

# SendGrid Email Configuration
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@example.com
SENDGRID_WELCOME_TEMPLATE_ID=d-your-template-id
SENDGRID_VERIFICATION_TEMPLATE_ID=d-your-template-id
SENDGRID_RESET_PASSWORD_TEMPLATE_ID=d-your-template-id
SENDGRID_QUOTE_REQUEST_TEMPLATE_ID=d-your-template-id
SENDGRID_BOOKING_CONFIRMATION_TEMPLATE_ID=d-your-template-id

# Optional: Analytics and Monitoring
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your-recaptcha-site-key

# Note: Never commit the actual .env file to version control
# Copy this file to .env.local and fill in the actual values 